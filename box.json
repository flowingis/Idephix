{
  "alias": "idephix.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Idephix\\Phar\\Compactor\\Php"
  ],
  "directories": [
    "src/Idephix"
  ],
  "extract": true,
  "files": [
    "README.md"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "tests",
        "test",
        "Test",
        "Tests"
      ],
      "in": "vendor"
    }
  ],
  "git-commit": "package_version",
  "git-version": "git-version",
  "datetime": "release_date",
  "main": "bin\/idx",
  "output": "idephix.phar",
  "stub": true
}